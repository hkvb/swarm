#!/usr/bin/env bash

# START CONFIGURATION
export VCW_STACK_GLOBAL_PACKAGE_CONTECO="hkvb/grafana/traefik"
export VCW_STACK_GLOBAL_PACKAGE_TAG="7.2.1"
export VCW_STACK_GLOBAL_STACKNAMES="grafana"
export VCW_STACK_GLOBAL_BOOTSTACKS=""
export VCW_STACK_GLOBAL_RUNSTACKS="grafana"
export VCW_STACK_GLOBAL_VOLUME_LIST="${VCW_STACK_GLOBAL_STACKPREFIX}_volume"
export VCW_STACK_GLOBAL_VOLUME_INITIALISED="${VCW_STACK_GLOBAL_STACKPREFIX}_volume:/var/lib/grafana"
# END CONFIGURATION

# START MODULE SETTINGS
export GRAFANA_ENVIRONMENT_GF_AUTH_ANONYMOUS_ORG_ROLE="${GRAFANA_MODULE_GRAFANA_AUTH_ANONYMOUS_ORG_ROLE}"
export GRAFANA_ENVIRONMENT_GF_LOG_LEVEL="${GRAFANA_MODULE_GRAFANA_LOG_LEVEL}"
# END MODULE SETTINGS

# START VOLUMES SETTINGS
# END VOLUMES SETTINGS

# START PORTS SETTINGS
# END PORTS SETTINGS

# START NETWORKS SETTINGS
# END NETWORKS SETTINGS

# START DEPLOY SETTINGS
export GRAFANA_DEPLOY_RESOURCES_LIMITS_MEMORY="${GRAFANA_MODULE_GRAFANA_MEMORY_LIMIT}"
export GRAFANA_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="${GRAFANA_MODULE_GRAFANA_MEMORY_RESERVED}"
# END DEPLOY SETTINGS


